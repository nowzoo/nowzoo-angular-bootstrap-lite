{
    "name": "nowzoo-angular-bootstrap-lite",
    "version": "0.2.35",
    "description": "A lightweight implementation of Bootstrap 4 for Angular 4.",
    "main": "./bundles/nowzoo-angular-bootstrap-lite.umd.js",
    "module": "./modules/nowzoo-angular-bootstrap-lite.es5.js",
    "es2015": "./modules/nowzoo-angular-bootstrap-lite.js",
    "scripts": {
        "build": "node build.js",
        "test": "karma start",
        "pack-lib": "npm pack ./dist",
        "publish-lib": "npm publish ./dist",
        "docs-serve": "cd ./docs-src && ng serve",
        "docs": "cd ./docs-src && ng build -aot --target=production --base-href '/nowzoo-angular-bootstrap-lite' --deploy-url https://nowzoo.github.io/nowzoo-angular-bootstrap-lite/ --output-path ../docs",
        "compodoc": "compodoc -p tsconfig.json --disablePrivateOrInternalSupport",
        "compodoc-serve": "compodoc -s -r 4040"
    },
    "typings": "./index.d.ts",
    "author": "Christopher Carson",
    "repository": {
        "type": "git",
        "url": "https://github.com/nowzoo/nowzoo-angular-bootstrap-lite.git"
    },
    "bugs": {
        "url": "https://github.com/nowzoo/nowzoo-angular-bootstrap-lite/issues"
    },
    "homepage": "https://github.com/nowzoo/nowzoo-angular-bootstrap-lite",
    "keywords": [
        "angular",
        "javascript",
        "typescript"
    ],
    "license": "MIT",
