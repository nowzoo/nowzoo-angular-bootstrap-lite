        "@angular/platform-browser-dynamic": "^4.0.0",
        "@angular/platform-server": "^4.0.0",
        "@compodoc/compodoc": "1.0.0-beta.10",
        "@types/jasmine": "2.5.52",
        "@types/node": "8.0.1",
        "bootstrap": "^4.0.0-alpha.6",
        "chalk": "1.1.3",
        "codelyzer": "3.1.1",
        "core-js": "2.4.1",
